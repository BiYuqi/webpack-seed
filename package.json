{
  "name": "webpack-seed",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --open --inline --mode development --config build/webpack.dev.config",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.config.js",
    "build:git": "cross-env NODE_ENV=production env_config=git webpack --config build/webpack.prod.config.js",
    "analyzer": "cross-env NODE_ENV=production npm_config_report=true npm run build",
    "test": "karma start",
    "lint": "eslint -c .eslintrc.js 'src/**/*.js' 'build/**/*.js'",
    "fix": "eslint --fix 'src/**/*.js' 'build/**/*.js'",
    "prettier": "prettier --write 'src/**/*.js'",
    "ci": "npm run lint && npm run prettier && npm run test",
    "new": "node ./bin/index.js"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.{js,json,md,scss}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BiYuqi/webpack-seed.git"
  },
  "author": "BiYuqi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BiYuqi/webpack-seed/issues"
  },
  "homepage": "https://github.com/BiYuqi/webpack-seed#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^0.1.19",
    "commander": "^2.20.3",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.1",
    "css-loader": "^1.0.0",
    "ejs-loader": "^0.3.5",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-prettier": "^3.1.2",
    "file-loader": "^2.0.0",
    "glob": "^7.1.6",
    "handlebars": "^4.7.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "inquirer": "^6.5.2",
    "karma": "^3.1.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "lint-staged": "^10.0.8",
    "mini-css-extract-plugin": "^0.4.3",
    "mocha": "^5.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^2.7.1",
    "sass": "^1.32.4",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.42.0",
    "webpack-auto-find-port": "0.0.5",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "axios": "^0.21.1"
  },
  "browserslist": [
    "> 1%",
    "Firefox ESR",
    "last 2 versions",
    "ie >= 9"
  ]
}
